type Post {
  id: ID! @unique
  createdAt: DateTime!
  updatedAt: DateTime!
  isPublished: Boolean! @default(value: "false")
  title: String!
  content: String!
  author: User!
}

type Subject {
  id: ID! @unique
  name:String!
}

type CollegeEntranceExam {
  id: ID! @unique
  province:Province!
  subject:Subject!
  culscore:Float!
  proscore:Float
  candidatenum:String!
  student:User!
}


type User {
  id: ID! @unique
  username: String! @unique
  password: String!
  name: String
  gender:String
  birthdaycalendar:String
  birthday:DateTime
  birthProvince:Province
  birthCity:City
  birthArea:Area
  birthStreet:Street
  birthVillage:Village
  uid:String! @unique
  token:String! @unique
  posts: [Post!]!
  createdAt: DateTime!
  updatedAt: DateTime!
  regStatus:RegStatus
  families:[Family!]!
}

type Person {
  id: ID! @unique
  name:String!
  user:User @unique
  families:[Family!]!
}

type Family {
  id: ID! @unique
  from:User!
  to:Person!
  relationship:String!
  spouse:Family
  status:String!
}

type Province {
  id:ID! @unique
  code:String! @unique
  name:String! @unique
  cities:[City!]!
  people:[User!]!
}

type City {
  id:ID! @unique
  code:String! @unique
  name:String!
  province:Province!
  areas:[Area!]!
  people:[User!]!
}

type Area {
  id:ID! @unique
  code:String! @unique
  name:String!
  city:City!
  towns:[Street!]!
  people:[User!]!
}

type Street {
  id:ID! @unique
  code:String! @unique
  name:String!
  Area:Area!
  villages:[Village!]!
  people:[User!]!
}

type Village {
  id:ID! @unique
  code:String! @unique
  name:String!
  street:Street!
  people:[User!]!
}

type Education {
  id:ID! @unique
  name:String! @unique
}

type Major {
  id: ID! @unique
  name: String!
  category:String!
  education:Education!
  code:String
}

type University {
  id: ID! @unique
  name: String! @unique
  education:Education!
  identifier:String! @unique
  city:City!
}

type RegStatus {
  id: ID! @unique
  university:University
  major:Major!
  applicants:[User!]!
}
